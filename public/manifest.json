{
  "name": "outlook-cal-opener",
  "version": "0.0.1",
  "description": "Automatically opens the meeting URL from an Outlook Calendar event.",
  "background": {
    "service_worker": "build/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 3,
  "permissions": ["alarms", "storage", "identity", "tabs"]
}
